Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\*\shell\ArchivistToolAddToArchive]
@="Add to archive"
"MultiSelectModel"="Single"

[HKEY_CLASSES_ROOT\*\shell\ArchivistToolAddToArchive\command]
@="\"C:\\Utils\\archivist_tool.exe\" \"%1\""

[HKEY_CLASSES_ROOT\Directory\shell\ArchivistToolAddToArchive]
@="Add to archive"
"MultiSelectModel"="Single"

[HKEY_CLASSES_ROOT\Directory\shell\ArchivistToolAddToArchive\command]
@="\"C:\\Utils\\archivist_tool.exe\" \"%1\""